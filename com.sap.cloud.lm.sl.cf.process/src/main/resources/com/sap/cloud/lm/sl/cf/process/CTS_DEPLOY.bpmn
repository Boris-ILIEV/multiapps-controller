    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway" default="deployNextMtar"></exclusiveGateway>
    <sequenceFlow id="flow62" sourceRef="exclusivegateway1" targetRef="exclusivegateway3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${(Step_StartMtaDeploySubProcessStep == "SUCCESS") || (Step_StartMtaDeploySubProcessStep == "FAILED")}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="incrementIndexFlow" sourceRef="setCtsArchiveExtensionsTask" targetRef="incrementIndexTask"></sequenceFlow>
    <sequenceFlow id="flow64" sourceRef="incrementIndexTask" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow65" sourceRef="startDeploySubProcessTask" targetRef="monitorDeployProcessTask"></sequenceFlow>
    <sequenceFlow id="retryFlow" name="On Retry" sourceRef="exclusivegateway1" targetRef="startDeploySubProcessTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${(Step_StartMtaDeploySubProcessStep == "LOGICAL_RETRY")}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway" default="successFlow"></exclusiveGateway>
    <sequenceFlow id="successFlow" sourceRef="exclusivegateway3" targetRef="setCtsArchiveExtensionsTask"></sequenceFlow>
    <sequenceFlow id="failureFlow" sourceRef="exclusivegateway3" targetRef="incrementIndexTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${(Step_StartMtaDeploySubProcessStep == "FAILED")}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="deployNextMtar" sourceRef="exclusivegateway2" targetRef="startDeploySubProcessTask"></sequenceFlow>
